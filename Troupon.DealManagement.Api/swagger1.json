          "400": {
            "description": "Returned if the model couldn't be parsed or the customer couldn't be found",
            "schema": {
              "$ref": "#/definitions/ValidationProblemDetails"
            }
          },
          "406": {
            "description": "Returned if no response found with an acceptable format",
